<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://apps.axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://apps.axelor.com/xml/ns/object-views http://apps.axelor.com/xml/ns/object-views/object-views_0.9.xsd">
    
    <grid name="event-category-grid" title="Event categories" model="com.axelor.apps.crm.db.EventCategory">
        <field name="name"/>
    </grid>
    
    <grid name="event-category-conf-grid" title="Event configuration categories" model="com.axelor.apps.crm.db.EventCategory">
        <field name="name"/>
        <field name="code"/>
        <field name="typeSelect"/>
    </grid>
    
    <form name="event-category-form" title="Event category" model="com.axelor.apps.crm.db.EventCategory" cols="2">
        <field name="name"/>
        <field name="code"/>
        <field name="typeSelect"/>
    </form>
    
</object-views>